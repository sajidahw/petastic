{
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "https://petastic.vercel.app/login"
    },
    {
      "src": "/register",
      "dest": "https://petastic.vercel.app/register"
    },
    {
      "src": "/pet",
      "dest": "https://petastic.vercel.app/pet"
    },
    {
      "src": "/pet/:id",
      "dest": "https://petastic.vercel.app/pet/:id"
    },
    {
      "src": "/pets",
      "dest": "https://petastic.vercel.app/pets"
    },
    {
      "src": "/faq",
      "dest": "https://petastic.vercel.app/faq"
    },
    {
      "src": "/forgot",
      "dest": "https://petastic.vercel.app/forgot"
    },

    {
      "src": "/(.*)",
      "dest": "http://localhost:8181/"
    },
    {
      "src": "/(.*)",
      "dest": "https://petastic.vercel.app/"
    }
  ],
  "rewrites": [
    {
      "source": "/:path((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
