{
  "rewrites": [
    {
      "source": "./:path((?!/).*)",
      "destination": "./index.html"
    }
  ],
  "routes": [
    {
      "src": "./login",
      "dest": "./login"
    },
    {
      "src": "./register",
      "dest": "./register"
    },
    {
      "src": "./pet",
      "dest": "./pet"
    },
    {
      "src": "./pet/:id",
      "dest": "./pet/:id"
    },
    {
      "src": "./pets",
      "dest": "./pets"
    },
    {
      "src": "./faq",
      "dest": "./faq"
    },
    {
      "src": "./forgot",
      "dest": "./forgot"
    },

    {
      "src": "./(.*)",
      "dest": "./index.html"
    }
  ]
}
